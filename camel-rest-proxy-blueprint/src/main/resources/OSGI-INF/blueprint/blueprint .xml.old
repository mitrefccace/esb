<?xml version="1.0" encoding="UTF-8"?>
<!-- camel proxy example -->

<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
           http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

	<bean id="jetty" class="org.apache.camel.component.jetty.JettyHttpComponent">
		<property name="sslSocketConnectorProperties">
			<map>
				<entry key="password" value="xxxxxxxx" />
				<entry key="keyPassword" value="xxxxxxxx" />
				<entry key="keystore" value="fullfilepath" />
				<entry key="needClientAuth" value="false" />
			</map>
		</property>
	</bean>

	<camelContext xmlns="http://camel.apache.org/schema/blueprint">
		<route id="rest-zendesk">
			<from uri="jetty:https://0.0.0.0?matchOnUriPrefix=true" />
			<log message="from test: route: CamelHttpPath=[${header.CamelHttpPath}], CamelHttpUri=[${header.CamelHttpUri}]" />
			<to uri="jetty:https://user.host.com?bridgeEndpoint=true&amp;throwExceptionOnFailure=false&amp;proxyHost=proxy.host.com&amp;proxyPort=00" />
			<log message="to test:" />
		</route>
	</camelContext>
</blueprint>